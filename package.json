{
  "name": "tickk-voice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "export": "npm run build && next export",
    "clean": "rm -rf .next out",
    "test:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "sitemap": "next-sitemap",
    "prebuild": "npm run type-check",
    "features:on": "node scripts/toggle-features.js on",
    "features:off": "node scripts/toggle-features.js off",
    "features:status": "node scripts/toggle-features.js status"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@stripe/stripe-js": "^4.9.0",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "compromise": "^14.14.0",
    "critters": "^0.0.23",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "gtag": "^1.0.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.548.0",
    "next": "^16.0.7",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.8.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "stripe": "^17.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "overrides": {
    "tailwindcss": "^3.4.1"
  }
}
